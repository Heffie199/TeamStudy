<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.team.youdao.bean">
	<class name="NoteBookGroup" table="tcnotebookgroup">
		<id name="noteBookGroupId" column="NoteBookGroup_Id">
			<!-- oracle用sequence创建主键规范指定sequence名 -->
			<generator class="sequence">
				<param name="sequence">seq_human</param>
			</generator>
		</id>
		<property name="noteBookGroupName"  column="noteBook_GroupName" type="java.lang.String"></property>
		<property name="textSum"  column="text_Sum" type="java.lang.Integer"></property>
		<property name="createdate" column="create_date" type="java.util.Date" ></property>
		<property name="flag" not-null="true" column="flag" type="java.lang.Integer"></property>
	</class>
</hibernate-mapping>
